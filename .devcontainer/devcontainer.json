{
	"name": "Chatbot Node.js project",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"remoteUser": "node",
	"forwardPorts": [
		5173,
		4001,
		8097,
		3000
	],
	"remoteEnv": {
		"CLIENT_HOST": "localhost",
		"CLIENT_PORT": "5173",
		"API_PORT": "4001",
		"NODE_ENV" : "development"
	}
}
